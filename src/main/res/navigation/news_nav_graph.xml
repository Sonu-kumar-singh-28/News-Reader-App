<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/news_nav_graph"
    app:startDestination="@id/headlinesFragment2">

    <fragment
        android:id="@+id/articlesFragment"
        android:name="com.ssu.portfolio.newsreaderapp.ui.Fragements.ArticlesFragment"
        android:label="ArticlesFragment" />
    <fragment
        android:id="@+id/favoritesFragment"
        android:name="com.ssu.portfolio.newsreaderapp.ui.Fragements.FavoritesFragment"
        android:label="FavoritesFragment" >
        <action
            android:id="@+id/action_favoritesFragment_to_articlesFragment"
            app:destination="@id/articlesFragment" />
    </fragment>
    <fragment
        android:id="@+id/headlinesFragment2"
        android:name="com.ssu.portfolio.newsreaderapp.ui.Fragements.HeadlinesFragment"
        android:label="HeadlinesFragment" >
        <action
            android:id="@+id/action_headlinesFragment2_to_articlesFragment"
            app:destination="@id/articlesFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment2"
        android:name="com.ssu.portfolio.newsreaderapp.ui.Fragements.SearchFragment"
        android:label="SearchFragment" >
        <action
            android:id="@+id/action_searchFragment2_to_articlesFragment"
            app:destination="@id/articlesFragment" />
    </fragment>
</navigation>